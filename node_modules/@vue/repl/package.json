    "url": "https://github.com/vuejs/repl/issues"
  },
  "homepage": "https://github.com/vuejs/repl#readme",
  "devDependencies": {
    "@babel/types": "^7.15.6",
    "@microsoft/api-extractor": "^7.19.2",
    "@types/codemirror": "^5.60.2",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-vue": "^3.0.0",
    "codemirror": "^5.62.3",
    "fflate": "^0.7.3",
    "hash-sum": "^2.0.0",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.1",
    "typescript": "^4.5.4",
    "vite": "^3.1.0",
    "vue": "^3.2.39",
    "vue-tsc": "^0.40.13"
  },
  "peerDependencies": {
    "vue": "^3.2.13"
  }
